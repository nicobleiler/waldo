{
  "name": "waldo.vision",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "turbo run clean",
    "checks": "turbo run build lint",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-custom": "workspace:0.0.0",
    "prettier": "latest",
    "tsconfig": "workspace:0.0.0",
    "turbo": "latest",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.15.0"
}
